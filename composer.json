{
    "name": "waynestate/nova-ckeditor4-field",
    "description": "This nova package allows you to use CKEditor 4 for text areas.",
    "keywords": [
        "laravel",
        "nova",
        "ckeditor"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Waynestate\\Nova\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Waynestate\\Nova\\CKEditorFieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
